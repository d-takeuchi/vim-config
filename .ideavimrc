set incsearch
set ignorecase
set smartcase
set number
set relativenumber
set clipboard+=unnamedplus
set keep-english-in-normal

let mapleader="\<Space>"
" reload vimrc
nnoremap <leader><leader> :source ~/.ideavimrc<cr>
" enabled
set NERDTree
set commentary
" tab
nnoremap <C-l> gt
nnoremap <C-h> gT
" NERDTree
nmap <C-e> :NERDTree<CR>